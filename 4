#include "stdafx.h"
#include "iostream"
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	int n, m;
	cin >> n;
	cin >> m;
	int *mat_s = new int[n];
	int *mat_p = new int[n];
	int **mat = new int*[m];
	for (int i = 0; i < m; i++){
		mat[i] = new int[n];
	}
	for (int i = 0; i < m; i++){
		for (int j = 0; j < n; j++){
			cin >> mat[i][j];
		}
	}
	for (int i = 0; i < n; i++){
		mat_s[i] = 0;
		mat_p[i] = 0;
	}
	for (int i = 0; i < m; i++){

		for (int j = 0; j < n; j++){
			if (mat[i][j]>0){
				mat_s[j] += mat[i][j];
				mat_p[j]++;
			}
			cout << "[" << i << "][" << j << "]= " << mat[i][j] << "\t";
		}
		cout << endl;
	}
	for (int i = 0; i < n; i++){
		cout << mat_s[i] << " ";
	}
	cout << endl;
	for (int i = 0; i < n; i++){
		cout << mat_p[i] << " ";
	}

	for (int i = 0; i < m; i++){
		delete[]mat[i];
	}
	delete[]mat;
	return 0;
}
