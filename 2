#include "stdafx.h"
#include "iostream"
using namespace std;


int main()
{
	setlocale(LC_ALL, "ru");
	int n,max,x,y;
	cin >> n;
	int **m = new int*[n];
	for (int i = 0; i < n; i++){
		m[i] = new int[n];
	}
	for (int i = 0; i < n; i++){
		for (int j = 0; j < n; j++){
			cin >> m[i][j];
		}
	}
	for (int i = 0; i < n; i++){
		max = m[i][0];
		for (int j = 0; j < n; j++){
			if (max < m[i][j]){
				max = m[i][j];
				x = i;
				y = j;
			}
		}
		m[x][y] = m[i][i];
		m[i][i] = max;
		cout << endl;
	}
	for (int i = 0; i < n; i++){
		for (int j = 0; j < n; j++){
			cout << "[" << i << "][" << j << "]= " << m[i][j] << "\t";
		}
		cout << endl;
	}
	for (int i = 0; i < n; i++){
		delete[]m[i];
	}
	delete[]m;
	return 0;
}
