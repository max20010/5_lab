#include "stdafx.h"
#include "iostream"
using namespace std;


int main()
{
	setlocale(LC_ALL, "ru");
	int n, x_min=0, y_min=0, x_max=0,y_max=0;
	cin >> n;
	int **m = new int*[n];
	for (int i = 0; i < n; i++){
		m[i] = new int[n];
	}
	for (int i = 0; i < n; i++){
		for (int j = 0; j < n; j++){
			cin >>m[i][j];
		}
	}
	int max = m[0][0], min = m[0][0];
	for (int i = 0; i < n; i++){
		for (int j = 0; j < n; j++){
			cout << "[" << i << "][" << j << "]= " << m[i][j] << "\t";
			if (m[i][j]>max){
				max = m[i][j];
				x_max = i;
				y_max = j;
			}
			if (m[i][j] < min){
				min = m[i][j];
				x_min = i;
				y_min = j;
			}
		}
		cout << endl;
	}
	cout << "max: [" << x_max << "][" << y_max << "]= " << max << endl;
	cout << "min: [" << x_min << "][" << y_min << "]= " << min << endl;
	for (int i = 0; i < n; i++){
		delete[]m[i];
	}
	delete[]m;
	return 0;
}
