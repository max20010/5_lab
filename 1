#include "stdio.h"
int main()
{
	int n, x_min=0, y_min=0, x_max=0,y_max=0;
	scanf("%d",&n);
	int m[n][n];
	for (int i = 0; i < n; i++){
		for (int j = 0; j < n; j++){
		scanf("%d",&m[i][j]);
		}
	}
	int max = m[0][0], min = m[0][0];
	for (int i = 0; i < n; i++){
		for (int j = 0; j < n; j++){
			printf("[%d][%d]= %d",i,j,m[i][j]);
			if (m[i][j]>max){
				max = m[i][j];
				x_max = i;
				y_max = j;
			}
			if (m[i][j] < min){
				min = m[i][j];
				x_min = i;
				y_min = j;
			}
		}
		printf("\n");
	}
	printf("max - [%d][%d]= %d",x_max,y_max,max);     
	printf("min - [%d][%d]= %d",x_min,y_min,min); 
	return 0;
}
